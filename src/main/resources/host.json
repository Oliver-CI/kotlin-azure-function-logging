{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.0.0, 5.0.0)"
  },
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Host.Aggregator": "Trace",
      "Host.Results": "Information",
      "Microsoft": "Warning",
      "Worker": "Information"
    }
  },
  "applicationInsights": {
    "samplingSettings": {
      "isEnabled": true,
      "maxTelemetryItemsPerSecond": 4,
      "excludedTypes": "Request;Exception"
    }
  }
}